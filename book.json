{
    "title": "leetcode",
    "author": "kabitonn",
    "description": "leetcode刷题",
    "language": "zh-hans",
    "links": {
        "gitbook": true
    },
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "katex",
        "splitter",
        "anchor-navigation-ex",
        "github-buttons",
        "copy-code-button",
        "-lunr",
        "-livereload",
        "-search",
        "search-plus",
        "emphasize",
        "expandable-chapters-small"
    ],
    "pluginsConfig": {
        "github-buttons": {
            "buttons": [
                {
                    "user": "kabitonn",
                    "repo": "leetcode-note",
                    "types": "star",
                    "size": "small",
                    "count": true
                }
            ]
        },
        "anchor-navigation-ex": {
            "multipleH1": false
        }
    },
    "pdf": {
        "fontSize": 16
    },
    "gitbook": "3.2.3"
}