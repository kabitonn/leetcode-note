{
    "title": "leetcode",
    "author": "kabitonn",
    "description": "leetcode刷题",
    "output.name": "site",
    "language": "zh-hans",
    "links": {
        "gitbook": true
    },
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "-sharing",
        "-lunr",
        "-highlight",
        "-livereload",
        "-search",
        "katex",
        "splitter",
        "disqus",
        "anchor-navigation-ex",
        "github",
        "github-buttons",
        "copy-code-button",
        "search-plus",
        "sharing-plus",
        "emphasize",
        "prism",
        "prism-themes",
        "expandable-chapters-small"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "leetcode note"
        },
        "github": {
            "url": "https://github.com/kabitonn/leetcode-note"
        },
        "github-buttons": {
            "buttons": [
                {
                    "user": "kabitonn",
                    "repo": "leetcode-note",
                    "type": "star",
                    "size": "small"
                }
            ]
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "anchor-navigation-ex": {
            "multipleH1": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper",
                "linkedin",
                "pocket",
                "stumbleupon",
                "qq",
                "qzone"
            ]
        }
    },
    "pdf": {
        "fontSize": 16
    },
    "gitbook": "3.2.3"
}